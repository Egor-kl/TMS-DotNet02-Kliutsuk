@{
    Layout = null;
}
<!DOCTYPE html>
<html lang='en'>
<head>
    <meta charset='utf-8' />
    <link href='~/fullcalendarIo/main.css' rel='stylesheet' />
    <link href='~/lib/bootstrap/dist/css/bootstrap.css' rel='stylesheet' />
    <link href='jquery/jquery-ui.css' rel='stylesheet' />
    <script src='fc/lib/main.js'></script>
    <script src="~js/app.js"></script>
    <script src="//code.jquery.com/jquery-1.10.2.js"></script>
    <script src="//code.jquery.com/ui/1.11.4/jquery-ui.js"></script>
    <script src="js/jquery-ui.css"></script>
    <script></script>
</head>
<body>
    <div id='calendar'></div>
    <div id="dialog">
        <form method="post" asp-controller="Calendar" asp-action="Calendar">
            <div class="form-group">
                <label for="title">Title</label>
                <input type="text" class="form-control" id="title" placeholder="title">
            </div>
            <div class="form-group">
                <label for="Date">Date</label>
                <input type="text" class="form-control datepicker" id="Date" placeholder="Date">
            </div>
            <div class="form-group">
                <label class="Place">Place</label>
                <input type="text" class="form-control" id="Place" placeholder="Place">
            </div>
            <div class="form-group">
                <label class="Note">Description</label>
                <input type="text" class="form-control" id="Note" placeholder="Description">
            </div>
            <div class="form-group">
                <label class="Email" rows="3">Emails of participants</label>
                <textarea type="text" class="form-control" id="Emails" placeholder="Emails of participants, for example: some@some.com , some2@some.com , some3@some.com "></textarea>
            </div>
            <button type="submit" class="btn btn-success">Add event</button>
        </form>
    </div>
    <script src="js/app.js">
    </script>
</body>